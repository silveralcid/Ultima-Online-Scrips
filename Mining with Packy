# set packy as organizer #1 and set gems to transfer

if findtype 'shovel'
    dclicktype 'shovel'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
elseif findtype 'pickaxe'
    dclicktype 'pickaxe'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
endif
if insysmsg 'There is no metal here to mine.'
    walk left
    walk left
    walk left
    walk left
    walk left
    walk left
    walk left
    walk left
    walk left
endif
if findtype 'shovel'
    dclicktype 'shovel'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
elseif findtype 'pickaxe'
    dclicktype 'pickaxe'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
endif
if insysmsg 'There is no metal here to mine.'
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
    walk down
endif
if findtype 'shovel'
    dclicktype 'shovel'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
elseif findtype 'pickaxe'
    dclicktype 'pickaxe'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
endif
if insysmsg 'There is no metal here to mine.'
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
    walk right
endif
if findtype 'shovel'
    dclicktype 'shovel'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
elseif findtype 'pickaxe'
    dclicktype 'pickaxe'
    wft
    targetrelloc 1 1
    pause 1000
    if weight > 250
        organizer 1
    endif
    pause 500
endif
if insysmsg 'There is no metal here to mine.'
    walk up
    walk up
    walk up
    walk up
    walk up
    walk up
